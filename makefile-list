# makefile for python.c
# 04-20-2018

CC=gcc
CFLAGS=-Wall -g -c

all: dc-tests

dc-tests: dc-tests.o dict.o emalloc.o
	$(CC) dc-tests.o dict.o emalloc.o -o dc-tests
	
dc-tests.o: dc-tests.c dict.h
	$(CC) $(CFLAGS) dc-tests.c

dict.o: dict.c dict.h emalloc.h
	 $(CC) $(CFLAGS) dict.c
	
emalloc.o: emalloc.c emalloc.h
	$(CC) $(CFLAGS) emalloc.c

clean:
	rm *.o dc-tests

